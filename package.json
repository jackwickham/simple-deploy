{
  "name": "simple-deploy",
  "version": "1.0.0",
  "private": true,
  "description": "Create a github deployment and use it to deploy main when new commits are made",
  "author": "Jack Wickham",
  "license": "MIT",
  "homepage": "https://github.com/jackwickham/simple-deploy",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node --async-stack-traces ./lib/index.js",
    "test": "jest",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "check": "npm run format-check && npm run lint && npm run test",
    "all": "npm run build && npm run format && npm run lint -- --fix && npm run test"
  },
  "dependencies": {
    "@octokit/rest": "^18.12.0",
    "js-yaml": "^4.1.0",
    "pino": "^7.6.3",
    "probot": "^12.2.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.178",
    "@types/mock-fs": "^4.13.1",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^8.6.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^25.7.0",
    "jest": "^27.4.7",
    "lodash": "^4.17.21",
    "mock-fs": "^5.1.2",
    "nock": "^13.2.2",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">= 12.0.0"
  }
}
