{
  "name": "simple-deploy",
  "version": "1.0.0",
  "private": true,
  "description": "Create a github deployment and use it to deploy main when new commits are made",
  "author": "Jack Wickham",
  "license": "MIT",
  "homepage": "https://github.com/jackwickham/simple-deploy",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node --async-stack-traces ./lib/index.js",
    "test": "jest",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "check": "npm run format-check && npm run lint && npm run test",
    "all": "npm run build && npm run format && npm run lint -- --fix && npm run test"
  },
  "dependencies": {
    "@octokit/rest": "^18.12.0",
    "js-yaml": "^4.1.0",
    "pino": "^7.2.0",
    "probot": "^12.1.1"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/js-yaml": "^4.0.4",
    "@types/lodash": "^4.14.177",
    "@types/mock-fs": "^4.13.1",
    "@types/node": "^16.11.8",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.2.4",
    "jest": "^26.6.3",
    "lodash": "^4.17.21",
    "mock-fs": "^5.1.2",
    "nock": "^13.2.1",
    "prettier": "^2.4.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.5.2"
  },
  "engines": {
    "node": ">= 12.0.0"
  }
}
