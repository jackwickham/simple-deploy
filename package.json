{
  "name": "simple-deploy",
  "version": "1.0.0",
  "private": true,
  "description": "Create a github deployment and use it to deploy main when new commits are made",
  "author": "Jack Wickham",
  "license": "MIT",
  "homepage": "https://github.com/jackwickham/simple-deploy",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node --async-stack-traces ./lib/index.js",
    "test": "jest",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "check": "npm run format-check && npm run lint && npm run test",
    "all": "npm run build && npm run format && npm run lint -- --fix && npm run test"
  },
  "dependencies": {
    "@octokit/rest": "^18.7.1",
    "js-yaml": "^4.1.0",
    "pino": "^6.13.0",
    "probot": "^12.1.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/js-yaml": "^4.0.2",
    "@types/lodash": "^4.14.171",
    "@types/mock-fs": "^4.13.1",
    "@types/node": "^16.4.3",
    "@types/pino": "^6.3.10",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "eslint": "^7.31.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.4.0",
    "jest": "^26.6.3",
    "lodash": "^4.17.21",
    "mock-fs": "^5.0.0",
    "nock": "^13.1.1",
    "prettier": "^2.3.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": ">= 12.0.0"
  }
}
